<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
		<langChildren>0</langChildren>
	</meta>
	<ROOT>
		<TCEforms>
			<sheetTitle>LLL:EXT:toctoc_comments/pi1/locallang.xml:sheet.attachments</sheetTitle>
			<cshFile>LLL:EXT:toctoc_comments/pi1/locallang_csh.xml</cshFile>
		</TCEforms>
		<type>array</type>
		<el>	
			<useWebpagePreview> 
				<TCEforms>
					<label>LLL:EXT:toctoc_comments/pi1/locallang.xml:tt_content.toctoc_comments_pi1.useWebpagePreview</label>
					<cshFile>LLL:EXT:toctoc_comments/pi1/locallang_csh.xml</cshFile>
					<config>
						<type>select</type>
						<items>
							<n0>
								<n0>LLL:EXT:toctoc_comments/pi1/locallang.xml:tt_content.toctoc_comments_pi1.usets</n0>
								<n1></n1>
							</n0>
							<n1>
								<n0>LLL:EXT:toctoc_comments/pi1/locallang.xml:tt_content.toctoc_comments_pi1.useno</n0>
								<n1>0</n1>
							</n1>
							<n2>
								<n0>LLL:EXT:toctoc_comments/pi1/locallang.xml:tt_content.toctoc_comments_pi1.useyes</n0>
								<n1>1</n1>
							</n2>
						</items>
						<default></default>
					</config>
				</TCEforms>
			</useWebpagePreview>
			<useTopWebpagePreview> 
				<TCEforms>
					<label>LLL:EXT:toctoc_comments/pi1/locallang.xml:tt_content.toctoc_comments_pi1.topwebpagepreviewlink</label>
					<config>
						<type>group</type>
						<eval>trim</eval>
						<default></default>
						<internal_type>db</internal_type>
						<prepand_tname>true</prepand_tname>
						<allowed>tx_toctoc_comments_attachment</allowed>
						<minsize>1</minsize>
						<maxsize>1</maxsize>
						<size>1</size>						
						<wizards type="array">
							<_PADDING>1</_PADDING>
							<_VERTICAL>1</_VERTICAL>
							<link type="array">
								<type>popup</type>
								<title>LLL:EXT:toctoc_comments/pi1/locallang.xml:toctoc_comments_pi1.topwebpagepreview.wizard</title>
								<icon>link_popup.gif</icon>
								<script>wizard_edit.php</script>
								<popup_onlyOpenIfSelected>1</popup_onlyOpenIfSelected>
								<icon>edit2.gif</icon>
								<JSopenParams>height=350,width=580,status=0,menubar=0,scrollbars=1</JSopenParams>
							</link>
						</wizards>
					</config>
				</TCEforms>
			</useTopWebpagePreview>
			<topWebpagePreviewPicture> 
				<TCEforms>
				<label>LLL:EXT:toctoc_comments/pi1/locallang.xml:tt_content.toctoc_comments_pi1.PreviewPictureNumber</label>
				<config>
					<type>input</type>
					<default></default>
					<eval>trim,lower,alphanum</eval>
					<size>2</size>
				</config>
				</TCEforms>
			</topWebpagePreviewPicture>
		</el>
	</ROOT>
</T3DataStructure>