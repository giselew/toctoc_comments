<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
		<langChildren>0</langChildren>
	</meta>
	<ROOT>
		<TCEforms>
		
			<sheetTitle>LLL:EXT:toctoc_comments/pi1/locallang.xml:sheet.advanced</sheetTitle>
			<cshFile>LLL:EXT:toctoc_comments/pi1/locallang_csh.xml</cshFile>
		</TCEforms>
		<type>array</type>
		<el>
			<commentReview>
				<TCEforms>
				<label>LLL:EXT:toctoc_comments/pi1/locallang.xml:tt_content.toctoc_comments_pi1.commentReview</label>
				<config>
					<type>select</type>
					<items>
						<n0>
							<n0>LLL:EXT:toctoc_comments/pi1/locallang.xml:tt_content.toctoc_comments_pi1.usets</n0>
							<n1></n1>
						</n0>
						<n1>
							<n0>LLL:EXT:toctoc_comments/pi1/locallang.xml:tt_content.toctoc_comments_pi1.useno</n0>
							<n1>0</n1>
						</n1>
						<n2>
							<n0>LLL:EXT:toctoc_comments/pi1/locallang.xml:tt_content.toctoc_comments_pi1.useyes</n0>
							<n1>1</n1>
						</n2>
					</items>
					<default></default>
				</config>
				</TCEforms>
			</commentReview>
			<closeCommentsAfter>
				<TCEforms>
				<label>LLL:EXT:toctoc_comments/pi1/locallang.xml:tt_content.toctoc_comments_pi1.closeCommentsAfter</label>
				<config>
					<type>input</type>
					<default></default>
					<eval>trim,lower,alphanum</eval>
					<size>15</size>
				</config>
				</TCEforms>
			</closeCommentsAfter>
			<commentingClosed> 
				<TCEforms>
				<label>LLL:EXT:toctoc_comments/pi1/locallang.xml:tt_content.toctoc_comments_pi1.commenting.closed</label>
				<config>
					<type>select</type>
					<items>
						<n0>
							<n0>LLL:EXT:toctoc_comments/pi1/locallang.xml:tt_content.toctoc_comments_pi1.usets</n0>
							<n1></n1>
						</n0>
						<n1>
							<n0>LLL:EXT:toctoc_comments/pi1/locallang.xml:tt_content.toctoc_comments_pi1.useno</n0>
							<n1>0</n1>
						</n1>
						<n2>
							<n0>LLL:EXT:toctoc_comments/pi1/locallang.xml:tt_content.toctoc_comments_pi1.useyes</n0>
							<n1>1</n1>
						</n2>
					</items>
					<default></default>
				</config>
				</TCEforms> 
			</commentingClosed>
			
			<initialViewsCount>
				<TCEforms>
				<label>LLL:EXT:toctoc_comments/pi1/locallang.xml:tt_content.toctoc_comments_pi1.initialViewsCount</label>
				<config>
					<type>input</type>
					<default></default>
					<eval>trim,lower,alphanum</eval>
					<size>15</size>
				</config>
				</TCEforms>
			</initialViewsCount>
			<initialViewsDate>
				<TCEforms>
				<label>LLL:EXT:toctoc_comments/pi1/locallang.xml:tt_content.toctoc_comments_pi1.initialViewsDate</label>
				<exclude>1</exclude>
					<config>
						<type>input</type>
						<size>10</size>
						<eval>date</eval>
						<checkbox>0</checkbox>
					</config>
				</TCEforms>
			</initialViewsDate>
		</el>
	</ROOT>
</T3DataStructure>